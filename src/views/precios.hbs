<table class="table">
  <thead>
    <tr>
      <th scope="col">Madera</th>
      <th scope="col">Precio</th>
    </tr>
  </thead>
  <tbody>
   {{#each maderas}}
    <tr>
     
      <td>{{name}}</td>
      <td>{{price}}</td>
      <td class="td-container">
          <a class="botonEditar" href="/edit/{{_id}}">Editar</a>
          <a class="botonEliminar" href="/delete/{{_id}}">Eliminar</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>




<a class="agregarVenta" href="/addMadera">Agregar madera</a>