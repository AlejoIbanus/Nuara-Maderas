<form action="/addMadera" method="POST">
  <div class="mb-3">
    <label for="name" class="form-label">Nombre de la madera</label>
    <input type="text" class="form-control" id="name" name="name">
  </div>
  <div class="mb-3">
    <label for="price" class="form-label">Precio</label>
    <input type="text" class="form-control" id="price" name="price">
  </div>
  <div class="boton-container ">
      <button type="submit" class="btn btn-primary agregarVenta">Agregar</button>
  </div>
</form>